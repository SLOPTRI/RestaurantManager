<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="mainPanel" prefHeight="600.0" prefWidth="1200.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ProductViewController">
   <children>
      <TableView fx:id="table" layoutX="680.0" layoutY="129.0" onMouseClicked="#selectProduct" prefHeight="394.0" prefWidth="470.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="252.0" text="NAME" />
          <TableColumn fx:id="priceColumn" minWidth="4.0" prefWidth="113.0" text="PRICE" />
            <TableColumn fx:id="quantityColumn" prefWidth="104.0" text="QUANTITY" />
        </columns>
      </TableView>
      <Label fx:id="mainText" layoutX="17.0" layoutY="42.0" prefHeight="38.0" prefWidth="1166.0" styleClass="mainText" text="                               PRODUCTS">
         <font>
            <Font name="Ubuntu Sans Mono Regular" size="30.0" />
         </font>
      </Label>
      <TextField fx:id="priceField" layoutX="106.0" layoutY="282.0" prefHeight="26.0" prefWidth="460.0" />
      <TextField fx:id="nameField" layoutX="106.0" layoutY="198.0" prefHeight="26.0" prefWidth="460.0" />
      <Label layoutX="313.0" layoutY="159.0" text="NAME">
         <font>
            <Font name="Ubuntu Sans Mono Regular" size="20.0" />
         </font>
      </Label>
      <Label layoutX="308.0" layoutY="241.0" text="PRICE">
         <font>
            <Font name="Ubuntu Sans Mono Regular" size="20.0" />
         </font>
      </Label>
      <Button fx:id="addButton" layoutX="106.0" layoutY="436.0" mnemonicParsing="false" onMouseClicked="#addProduct" prefHeight="86.0" prefWidth="139.0" text="ADD">
         <font>
            <Font name="Ubuntu Sans Mono Regular" size="14.0" />
         </font>
      </Button>
      <Button fx:id="editButton" layoutX="266.0" layoutY="436.0" mnemonicParsing="false" onMouseClicked="#editProduct" prefHeight="86.0" prefWidth="139.0" text="EDIT">
         <font>
            <Font name="Ubuntu Sans Mono Regular" size="14.0" />
         </font>
      </Button>
      <Button fx:id="delButton" layoutX="427.0" layoutY="436.0" mnemonicParsing="false" onMouseClicked="#delProduct" prefHeight="86.0" prefWidth="139.0" text="DEL">
         <font>
            <Font name="Ubuntu Sans Mono Regular" size="14.0" />
         </font>
      </Button>
      <TextField fx:id="quantityField" layoutX="106.0" layoutY="367.0" prefHeight="26.0" prefWidth="460.0" />
      <Label layoutX="291.0" layoutY="326.0" text="QUANTITY">
         <font>
            <Font name="Ubuntu Sans Mono Regular" size="20.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="17.0" layoutY="29.0" mnemonicParsing="false" onMouseClicked="#viewToMain" text="MAIN" />
   </children>
</AnchorPane>
